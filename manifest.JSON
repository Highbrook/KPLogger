{
    "manifest_version": 3,
    "version": "1.0",
    "name": "KP Logger",
    "description": "A tool used to save KP data to a spread sheet.",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "permissions": ["tabs","activeTab","scripting"],
    "host_permissions": [
        "*://*/*",
        "<all_urls>"
    ]
}